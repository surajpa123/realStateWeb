{
   "version": 2,
   "name": "real-estate",
   "builds": [
     { "src": "backEnd/index.js", "use": "@vercel/node" }
   ],
   "routes": [
     { "src": "/(.*)", "dest": "backEnd/index.js", "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
     "headers": {
       "Access-Control-Allow-Origin": "*"
     }
    
    }
   ]
 }
 